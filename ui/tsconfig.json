{
    "compilerOptions": {
        "lib": [
            "scripthost",
            "es6",
            "dom"
        ],
        "outDir": "./out/",
        "noImplicitAny": false,
        "sourceMap": true,
        "module": "esnext",
        "moduleResolution": "node",
        "target": "es5",
        "jsx": "react",
        "allowJs": true,
        "allowSyntheticDefaultImports": true,
        "baseUrl": "src",
        "paths": {
            "app": [
                "app/index"
            ]
        }
    }
}
